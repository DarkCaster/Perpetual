	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Package: annotate",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": ["annotate"],
		},
		{
			"name": "Launch Package: brief report",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": ["report","-t","brief"],
		},
		{
			"name": "Launch Package: implement",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": ["implement","-pr"],
		},
		{
			"name": "Launch Package: stash",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": ["stash"],
		},
		{
			"name": "Launch Package: doc draft",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": ["doc","-a","draft","-r","test.md"],
		}
	]
}